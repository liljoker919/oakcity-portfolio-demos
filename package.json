{
  "name": "oakcity-portfolio-demos",
  "version": "1.0.0",
  "description": "Astro-based portfolio demo websites for small business niches, deployed as static sites to AWS S3 and CloudFront for use on oakcitysoftwaresolutions.com.",
  "private": true,
  "type": "module",
  "workspaces": [
    "sites/*"
  ],
  "scripts": {
    "dev:cleaning": "npm run dev --workspace=sites/cleaning",
    "build:cleaning": "npm run build --workspace=sites/cleaning",
    "preview:cleaning": "npm run preview --workspace=sites/cleaning",
    "dev:handyman": "npm run dev --workspace=sites/handyman",
    "build:handyman": "npm run build --workspace=sites/handyman",
    "preview:handyman": "npm run preview --workspace=sites/handyman",
    "dev:coach": "npm run dev --workspace=sites/coach",
    "build:coach": "npm run build --workspace=sites/coach",
    "preview:coach": "npm run preview --workspace=sites/coach"
  },
  "keywords": [
    "astro",
    "portfolio",
    "static-sites",
    "monorepo"
  ],
